    <#--隐藏的音乐播放器-->
    <div id="my_music">
        <audio src="music/-1.mp3" autoplay id="_music_" loop></audio>
    </div>
    <#--得到项目的路径-->
    <input type="hidden" value="${my_path}" id="_path"/>
    <script type="text/javascript">
        $(function () {
            console.log("start");
            $("a").click(function () {
                var reg = /^[0-9]*$/;//正则表达式
                var id = $(this).attr("value");//得到music_id
                if (reg.test(id)) {
                    console.log(id);
                    var music = $("audio")[0];//得到audio标签
                    if (music != null) {
                        console.log("load_music");
                        music.setAttribute("src", "music/" + id + ".mp3");
                        console.log("play_music");
                    }
                }
            })
            console.log("end");
        })
    </script>
