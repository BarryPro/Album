        <script type="text/javascript">
            $(function () {
                console.log("start");
                $("a").click(function () {
                    reg = /^[0-9]*$/;
                    id = $(this).attr("id");
                    if(reg.test(id)){
                        console.log(id);
                        music = $("audio")[0];//得到audio标签
                        if(music!=null){
                            console.log(music);
                            music.pause();
                            console.log("pause");
                        }

                        $("#my_music").append(
                            '<audio src="music/'+id+'.mp3" autoplay  id="_music_"></audio>'
                        )
                        console.log("play");
                    }
                })
                console.log("end");
            })
